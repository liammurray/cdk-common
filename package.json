{
  "name": "@liammurray/cdk-common",
  "version": "0.1.0",
  "description": "Common CDK shared code",
  "main": "./dist/src/index.js",
  "typings": "./dist/index.d.ts",
  "keywords": [
    "cdk",
    "typescript",
    "@liammurray/cdk-common"
  ],
  "scripts": {
    "audit-checker": "check-audit",
    "audit-resolver": "resolve-audit",
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "ttsc",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --write '**/*.{ts,js,md,json}'",
    "snapshot": "jest --updateSnapshot",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "npm run lint:mdlint && npm run lint:eslint",
    "lint:mdlint": "[ -f .markdownlint.yaml ] && markdownlint .",
    "lint:eslint": "eslint --cache --ext .ts ./"
  },
  "dependencies": {
    "@aws-cdk/aws-codebuild": "^1.44.0",
    "@aws-cdk/aws-codecommit": "^1.44.0",
    "@aws-cdk/aws-codepipeline": "^1.44.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.44.0",
    "@aws-cdk/aws-lambda": "^1.44.0",
    "@aws-cdk/aws-s3": "^1.44.0",
    "@aws-cdk/core": "^1.44.0"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.44.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cpx": "^1.5.1",
    "@types/eslint": "^6.8.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/markdownlint": "^0.13.0",
    "@types/node": "^14.0.11",
    "@types/nodemon": "^1.19.0",
    "@types/prettier": "^2.0.1",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.4",
    "@types/source-map-support": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "aws-cdk": "^1.44.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cpx": "^1.5.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.0.1",
    "markdownlint": "^0.20.3",
    "markdownlint-cli": "^0.23.1",
    "nodemon": "^2.0.4",
    "npm-audit-resolver": "^2.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.5",
    "typescript-eslint": "0.0.1-alpha.0",
    "typescript-transform-paths": "^1.1.14"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "github:liammurray/cdk-common",
  "files": [
    "dist",
    "package.json"
  ],
  "author": "Liam Murray",
  "license": "UNLICENSED",
  "readme": "./README.md"
}
